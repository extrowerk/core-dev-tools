#! /bin/sh
#
# QNX Neutrino include files should be good already - don't fix them
#
# See README-fixinc for more information.

# Directory in which to store the results.
LIB=${1?"fixincludes: output directory not specified"}

# Make sure it exists.
if [ ! -d $LIB ]; then
  mkdir $LIB || exit 1
fi

exit 0
